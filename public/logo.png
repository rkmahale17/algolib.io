<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 120 120" width="200" height="200" role="img" aria-labelledby="title desc">
  <title id="title">Algolib Logo</title>
  <desc id="desc">Minimal graph icon with Tailwind primary-to-secondary gradient background</desc>

  <!-- Gradient definition (matches your Tailwind setup) -->
  <defs>
    <linearGradient id="brand-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="hsl(262, 83%, 58%)"/>  <!-- from-primary -->
      <stop offset="100%" stop-color="hsl(217, 91%, 60%)"/> <!-- to-secondary -->
    </linearGradient>
  </defs>

  <!-- Background with gradient -->
  <rect width="120" height="120" rx="24" fill="url(#brand-gradient)" />

  <!-- Graph edges -->
  <g stroke="#ffffff" stroke-width="4" stroke-linecap="round" stroke-linejoin="round" fill="none">
    <path d="M30 80 L60 40" />
    <path d="M60 40 L90 70" />
    <path d="M60 40 L60 15" />
    <path d="M30 80 L50 100" />
  </g>

  <!-- Graph nodes -->
  <circle cx="30" cy="80" r="5" fill="#ffffff" />
  <circle cx="60" cy="40" r="5" fill="#ffffff" />
  <circle cx="90" cy="70" r="5" fill="#ffffff" />
  <circle cx="60" cy="15" r="5" fill="#ffffff" />
  <circle cx="50" cy="100" r="5" fill="#ffffff" />
</svg>
